{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base","prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parser":"@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
